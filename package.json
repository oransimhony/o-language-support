{
    "name": "o-language-support",
    "displayName": "O Language Support",
    "description": "Syntax Highlighter for the O Programming Language",
    "version": "0.0.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/oransimhony/o-language-support"
    },
    "publisher": "oransimhony",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "olang",
            "aliases": ["O", "olang"],
            "extensions": [".olang",".ol"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "olang",
            "scopeName": "source.olang",
            "path": "./syntaxes/olang.tmLanguage.json"
        }]
    }
}